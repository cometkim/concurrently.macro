{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "incremental": true,
    "outDir": ".",
    "skipLibCheck": true,
    "noEmit": true
  },
  "exclude": [
    "*.d.ts"
  ]
}
